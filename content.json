      { "q": "Which has terminating decimal?", "options": [ "1/20", "1/3", "1/7", "1/11" ], "answer": 0, "explanation": "Denominator factors 2 and 5 only." },
      { "q": "Sum rational + irrational =", "options": [ "rational", "irrational", "integer", "none" ], "answer": 1, "explanation": "Generally irrational." },
      { "q": "Is π rational?", "options": [ "Yes", "No", "Sometimes", "None" ], "answer": 1, "explanation": "π is irrational." },
      { "q": "Which gives repeating decimal?", "options": [ "1/9", "1/4", "1/2", "1/5" ], "answer": 0, "explanation": "1/9 = 0.111..." },
      { "q": "Square of irrational may be:", "options": [ "always irrational", "always rational", "may be either", "none" ], "answer": 2, "explanation": "e.g. √2 × √2 = 2 (rational)." },
