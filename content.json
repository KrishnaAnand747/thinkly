  "quizzes": {
    "Number Systems": [
      { "q": "Which of these is irrational?", "options": [ "0.75", "3/4", "√5", "2.0" ], "answer": 2, "explanation": "√5 is not expressible as p/q." },
      { "q": "1/3 as decimal is:", "options": [ "0.3", "0.33", "0.333...", "0.333" ], "answer": 2, "explanation": "Repeating 3." },
      { "q": "Which decimal terminates?", "options": [ "1/8", "1/3", "1/7", "1/6" ], "answer": 0, "explanation": "1/8 = 0.125" },
      { "q": "Between 3/7 and 4/7, there are:", "options": [ "no rational", "finite many", "exactly one", "infinitely many" ], "answer": 3, "explanation": "Infinitely many rationals exist." },
      { "q": "√2 is:", "options": [ "rational", "irrational", "integer", "whole" ], "answer": 1, "explanation": "Classic proof shows irrationality." },
      { "q": "Decimal of 1/5 is:", "options": [ "0.2", "0.25", "0.22", "0.5" ], "answer": 0, "explanation": "1/5 = 0.2" },
      { "q": "Product √2 × √8 =", "options": [ "√10", "√16", "4", "10" ], "answer": 2, "explanation": "=√16=4" },
      { "q": "Which is non-real?", "options": [ "π", "-5", "√2", "√(-1)" ], "answer": 3, "explanation": "Imaginary." },
      { "q": "Rational numbers are closed under:", "options": [ "addition", "multiplication", "subtraction", "all of the above" ], "answer": 3, "explanation": "All operations except division by zero." },
      { "q": "0 is:", "options": [ "natural", "whole", "irrational", "none" ], "answer": 1, "explanation": "Whole numbers include 0." },
      { "q": "0.2727... as fraction =", "options": [ "3/11", "27/99", "7/25", "1/4" ], "answer": 1, "explanation": "27/99 reduces to 3/11." },
      { "q": "Which has terminating decimal?", "options": [ "1/20", "1/3", "1/7", "1/11" ], "answer": 0, "explanation": "Denominator factors 2 and 5 only." },
      { "q": "Sum rational + irrational =", "options": [ "rational", "irrational", "integer", "none" ], "answer": 1, "explanation": "Generally irrational." },
      { "q": "Is π rational?", "options": [ "Yes", "No", "Sometimes", "None" ], "answer": 1, "explanation": "π is irrational." },
      { "q": "Which gives repeating decimal?", "options": [ "1/9", "1/4", "1/2", "1/5" ], "answer": 0, "explanation": "1/9 = 0.111..." },
      { "q": "Square of irrational may be:", "options": [ "always irrational", "always rational", "may be either", "none" ], "answer": 2, "explanation": "e.g. √2 × √2 = 2 (rational)." },
      { "q": "Between any two rationals exist:", "options": [ "no numbers", "finite numbers", "one number", "infinitely many" ], "answer": 3, "explanation": "Infinitely many rationals." },
      { "q": "Decimal of 7/12 is:", "options": [ "0.5833...", "0.58", "0.75", "0.7" ], "answer": 0, "explanation": "0.5833 repeating." },
      { "q": "Which is integer?", "options": [ "√4", "√2", "π", "1/3" ], "answer": 0, "explanation": "√4=2 integer." },
      { "q": "If a/b in lowest terms has denominator with primes only 2 and 5, decimal:", "options": [ "terminates", "repeats", "irrational", "none" ], "answer": 0, "explanation": "Property of decimal representation." }
